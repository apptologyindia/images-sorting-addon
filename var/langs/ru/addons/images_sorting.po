msgctxt "Addons::name::images_sorting"
msgid "Product sorting by Images"
msgstr "Сортировка продуктов по изображению"

msgctxt "Addons::description::images_sorting"
msgid "This addon is sorting products by images."
msgstr "Этот модуль сортирует товары исходя из наличия изображения."

msgctxt "SettingsOptions::images_sorting::sort_in_admin_panel"
